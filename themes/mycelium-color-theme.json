    "input.background": "#0F0F23",
    "input.foreground": "#D1D5DB",
    "input.border": "#374151",
    "input.placeholderForeground": "#9CA3AF",
    "inputOption.activeBorder": "#6366F1",
    "inputOption.activeBackground": "#6366F130",
    "inputValidation.errorBackground": "#7F1D1D",
    "inputValidation.errorBorder": "#F87171",
    "inputValidation.warningBackground": "#78350F",
