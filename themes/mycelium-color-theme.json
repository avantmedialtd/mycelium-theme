    "inputValidation.infoBorder": "#818CF8",

    "dropdown.background": "#1A1A2E",
    "dropdown.foreground": "#D1D5DB",
    "dropdown.border": "#374151",
    "dropdown.listBackground": "#1A1A2E",

    "button.background": "#6366F1",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#818CF8",
    "button.secondaryBackground": "#1A1A2E",
    "button.secondaryForeground": "#D1D5DB",
    "button.secondaryHoverBackground": "#374151",

    "badge.background": "#6366F1",
    "badge.foreground": "#FFFFFF",

    "scrollbar.shadow": "#00000050",
    "scrollbarSlider.background": "#37415180",
    "scrollbarSlider.hoverBackground": "#374151B0",
    "scrollbarSlider.activeBackground": "#6366F180",

    "progressBar.background": "#6366F1",

    "panel.background": "#0A0A1A",
    "panel.border": "#374151",
    "panelTitle.activeBorder": "#6366F1",
    "panelTitle.activeForeground": "#FFFFFF",
    "panelTitle.inactiveForeground": "#9CA3AF",

    "terminal.background": "#0A0A1A",
    "terminal.foreground": "#D1D5DB",
    "terminal.ansiBlack": "#0F0F23",
    "terminal.ansiRed": "#F87171",
    "terminal.ansiGreen": "#34D399",
    "terminal.ansiYellow": "#FBBF24",
    "terminal.ansiBlue": "#6366F1",
    "terminal.ansiMagenta": "#C084FC",
    "terminal.ansiCyan": "#22D3EE",
    "terminal.ansiWhite": "#D1D5DB",
    "terminal.ansiBrightBlack": "#9CA3AF",
    "terminal.ansiBrightRed": "#FCA5A5",
    "terminal.ansiBrightGreen": "#6EE7B7",
    "terminal.ansiBrightYellow": "#FDE68A",
    "terminal.ansiBrightBlue": "#818CF8",
    "terminal.ansiBrightMagenta": "#D8B4FE",
    "terminal.ansiBrightCyan": "#67E8F9",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminalCursor.foreground": "#6366F1",

    "peekView.border": "#6366F1",
    "peekViewEditor.background": "#0F0F23",
    "peekViewEditor.matchHighlightBackground": "#6366F140",
    "peekViewResult.background": "#0A0A1A",
    "peekViewResult.fileForeground": "#FFFFFF",
    "peekViewResult.lineForeground": "#D1D5DB",
    "peekViewResult.matchHighlightBackground": "#6366F140",
    "peekViewResult.selectionBackground": "#6366F130",
    "peekViewTitle.background": "#1A1A2E",
    "peekViewTitleDescription.foreground": "#9CA3AF",
    "peekViewTitleLabel.foreground": "#FFFFFF",

    "merge.currentHeaderBackground": "#34D39930",
    "merge.currentContentBackground": "#34D39910",
    "merge.incomingHeaderBackground": "#6366F130",
    "merge.incomingContentBackground": "#6366F110",

    "gitDecoration.addedResourceForeground": "#34D399",
    "gitDecoration.modifiedResourceForeground": "#818CF8",
    "gitDecoration.deletedResourceForeground": "#F87171",
    "gitDecoration.untrackedResourceForeground": "#34D399",
    "gitDecoration.ignoredResourceForeground": "#4B5563",
    "gitDecoration.conflictingResourceForeground": "#FBBF24",
    "gitDecoration.stageModifiedResourceForeground": "#818CF8",
    "gitDecoration.stageDeletedResourceForeground": "#F87171",

    "diffEditor.insertedTextBackground": "#34D39915",
    "diffEditor.removedTextBackground": "#F8717115",
    "diffEditor.insertedLineBackground": "#34D39910",
    "diffEditor.removedLineBackground": "#F8717110",

    "notificationCenter.border": "#374151",
    "notificationCenterHeader.background": "#1A1A2E",
    "notificationCenterHeader.foreground": "#FFFFFF",
    "notifications.background": "#1A1A2E",
    "notifications.foreground": "#D1D5DB",
    "notifications.border": "#374151",
    "notificationsErrorIcon.foreground": "#F87171",
    "notificationsWarningIcon.foreground": "#FBBF24",
    "notificationsInfoIcon.foreground": "#818CF8",

    "breadcrumb.foreground": "#9CA3AF",
    "breadcrumb.focusForeground": "#D1D5DB",
    "breadcrumb.activeSelectionForeground": "#FFFFFF",
    "breadcrumbPicker.background": "#1A1A2E",

    "minimap.findMatchHighlight": "#6366F180",
    "minimap.selectionHighlight": "#6366F140",
    "minimapGutter.addedBackground": "#34D399",
    "minimapGutter.modifiedBackground": "#818CF8",
    "minimapGutter.deletedBackground": "#F87171",

    "editorBracketHighlight.foreground1": "#6366F1",
    "editorBracketHighlight.foreground2": "#C084FC",
    "editorBracketHighlight.foreground3": "#22D3EE",
    "editorBracketHighlight.foreground4": "#FBBF24",
    "editorBracketHighlight.foreground5": "#34D399",
    "editorBracketHighlight.foreground6": "#F87171",

    "editorWidget.background": "#1A1A2E",
    "editorWidget.foreground": "#D1D5DB",
    "editorWidget.border": "#374151",
    "editorSuggestWidget.background": "#1A1A2E",
    "editorSuggestWidget.border": "#374151",
    "editorSuggestWidget.foreground": "#D1D5DB",
    "editorSuggestWidget.highlightForeground": "#6366F1",
    "editorSuggestWidget.selectedBackground": "#6366F130",

    "keybindingLabel.background": "#1A1A2E",
    "keybindingLabel.foreground": "#D1D5DB",
    "keybindingLabel.border": "#374151",
    "keybindingLabel.bottomBorder": "#374151",

    "focusBorder": "#6366F180",
    "widget.shadow": "#00000040",
    "selection.background": "#6366F140",
    "descriptionForeground": "#9CA3AF",
    "icon.foreground": "#9CA3AF",
    "foreground": "#D1D5DB"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#4B5563",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Doc Comments",
      "scope": [
        "comment.block.documentation",
        "comment.block.documentation storage.type",
        "comment.block.documentation punctuation.definition"
      ],
      "settings": {
        "foreground": "#6B7280",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.quoted.single",
        "string.quoted.double",
        "string.quoted.triple"
      ],
      "settings": {
        "foreground": "#34D399"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape", "string.quoted.template"],
      "settings": {
        "foreground": "#6EE7B7"
      }
    },
    {
      "name": "Template String Expressions",
      "scope": [
        "string.template punctuation.definition.template-expression",
        "string.template meta.template.expression"
      ],
      "settings": {
        "foreground": "#C084FC"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp",
        "string.regexp keyword.other",
        "string.regexp punctuation"
      ],
      "settings": {
        "foreground": "#22D3EE"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FBBF24"
      }
    },
    {
      "name": "Built-in Constants",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#FBBF24",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Other Constants",
      "scope": ["constant.other"],
      "settings": {
        "foreground": "#FBBF24"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete",
        "keyword.operator.typeof",
        "keyword.operator.instanceof",
        "keyword.operator.void",
        "keyword.operator.in",
        "keyword.operator.of"
      ],
      "settings": {
        "foreground": "#C084FC"
      }
    },
    {
      "name": "Control Flow",
      "scope": [
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.flow",
        "keyword.control.trycatch",
        "keyword.control.switch"
      ],
      "settings": {
        "foreground": "#C084FC",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.default",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#C084FC"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.relational"
      ],
      "settings": {
        "foreground": "#818CF8"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#C084FC"
      }
    },
    {
      "name": "Type Declarations",
      "scope": [
        "storage.type.interface",
        "storage.type.type",
        "storage.type.enum",
        "storage.type.class",
        "storage.type.namespace"
      ],
      "settings": {
        "foreground": "#C084FC",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function Declarations",
      "scope": [
        "entity.name.function",
        "meta.function-call entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#6366F1"
      }
    },
    {
      "name": "Method Calls",
      "scope": [
        "meta.method-call entity.name.function",
        "entity.name.function.member"
      ],
      "settings": {
        "foreground": "#818CF8"
      }
    },
    {
      "name": "Built-in Functions",
      "scope": ["support.function.builtin"],
      "settings": {
        "foreground": "#818CF8"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type.class",
        "entity.name.type",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#22D3EE"
      }
    },
    {
      "name": "Interfaces and Type Aliases",
      "scope": [
        "entity.name.type.interface",
        "entity.name.type.alias",
        "entity.name.type.enum"
      ],
      "settings": {
        "foreground": "#22D3EE",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Inherited Classes",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#22D3EE",
        "fontStyle": "italic underline"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#D1D5DB"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#FCA5A5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property Access",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#93C5FD"
      }
    },
    {
      "name": "Destructured Variables",
      "scope": [
        "meta.object-binding-pattern-variable variable.object.property",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#D1D5DB"
      }
    },
    {
      "name": "Language Variables",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#F87171",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#9CA3AF"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#9CA3AF"
      }
    },
    {
      "name": "Tag Names",
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#F87171"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#FBBF24",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag Brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#9CA3AF"
      }
    },
    {
      "name": "JSX Component Tags",
      "scope": [
        "support.class.component.js",
        "support.class.component.tsx",
        "support.class.component.jsx"
      ],
      "settings": {
        "foreground": "#22D3EE"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#93C5FD"
      }
    },
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#D1D5DB"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#FBBF24"
      }
    },
    {
      "name": "CSS Units",
      "scope": ["keyword.other.unit.css"],
      "settings": {
        "foreground": "#FBBF24"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "string.json support.type.property-name"
      ],
      "settings": {
        "foreground": "#6366F1"
      }
    },
    {
      "name": "JSON Strings",
      "scope": ["string.quoted.double.json"],
      "settings": {
        "foreground": "#34D399"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "heading.1.markdown entity.name",
        "heading.2.markdown entity.name",
        "heading.3.markdown entity.name",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#6366F1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#FBBF24",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#C084FC",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#22D3EE"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code",
        "markup.raw"
      ],
      "settings": {
        "foreground": "#34D399"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#9CA3AF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List",
      "scope": ["markup.list punctuation.definition.list"],
      "settings": {
        "foreground": "#6366F1"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "meta.decorator",
        "meta.decorator entity.name.function",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#FBBF24",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Preprocessor",
      "scope": [
        "meta.preprocessor",
        "keyword.preprocessor"
      ],
      "settings": {
        "foreground": "#9CA3AF"
      }
    },
    {
      "name": "LINQ Keywords",
      "scope": ["keyword.query"],
      "settings": {
        "foreground": "#C084FC"
      }
    },
    {
      "name": "Attributes",
      "scope": ["meta.attribute"],
      "settings": {
        "foreground": "#FBBF24",
        "fontStyle": "italic"
      }
    },
    {
      "name": "SQL Keywords",
      "scope": [
        "keyword.other.DML.sql",
        "keyword.other.DDL.sql",
        "keyword.other.sql"
      ],
      "settings": {
        "foreground": "#C084FC"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#93C5FD"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#F87171",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#9CA3AF",
        "fontStyle": "strikethrough"
      }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#22D3EE",
    "type": "#22D3EE",
    "interface": {
      "foreground": "#22D3EE",
      "fontStyle": "italic"
    },
    "enum": "#22D3EE",
    "enumMember": "#FBBF24",
    "struct": "#22D3EE",
    "class": "#22D3EE",
    "parameter": {
      "foreground": "#FCA5A5",
      "fontStyle": "italic"
    },
    "variable": "#D1D5DB",
    "variable.readonly": "#D1D5DB",
    "property": "#93C5FD",
    "property.readonly": "#93C5FD",
    "function": "#6366F1",
    "method": "#818CF8",
    "macro": "#FBBF24",
    "keyword": "#C084FC",
    "modifier": "#C084FC",
    "comment": {
      "foreground": "#4B5563",
      "fontStyle": "italic"
    },
    "string": "#34D399",
    "number": "#FBBF24",
    "regexp": "#22D3EE",
    "operator": "#818CF8",
    "decorator": {
      "foreground": "#FBBF24",
      "fontStyle": "italic"
    },
    "typeParameter": {
      "foreground": "#22D3EE",
      "fontStyle": "italic"
    }
  }
}
